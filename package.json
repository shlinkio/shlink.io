  "repository": {
    "type": "git",
    "url": "https://github.com/shlinkio/shlink.io"
  },
  "scripts": {
    "lint": "next lint --dir src --dir test",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --colors",
    "test:ci": "npm run test -- --coverage --coverageReporters=text --coverageReporters=text-summary --coverageReporters=clover",
    "purge": "purgecss --config ./purgecss.config.js --output ./out/_next/static/css",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "sitemap": "next-sitemap",
    "export": "npm run build && npm run sitemap && next export",
    "export:prod": "npm run export && npm run purge"
  },
